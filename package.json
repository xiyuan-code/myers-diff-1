{
  "name": "myers-diff",
  "author": {
    "name": "Jamie Peabody",
    "email": "jamie.peabody@gmail.com",
    "url": ""
  },
  "version": "1.0.5",
  "description": "Implementation of the longest common subsequence (diff) algorithm.",
  "main": "dist/index.js",
  "keywords": [
    "diff",
    "merge",
    "longest-common-subsequence",
    "shortest-middle-snake",
    "compare",
    "patch"
  ],
  "scripts": {
    "build": "npm run-script build:lint && npm run-script test",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --compilers js:babel-core/register ./test --recursive -R list",
    "dist": "npm run-script clean && npm run-script build && npm run-script build:transpile && npm run-script example && npm run-script build:readme",
    "build:transpile": "rm -rf ./dist && mkdir ./dist && babel src --out-dir ./dist",
    "build:lint": "eslint src ./test",
    "build:readme": "node-readme && mv README.md README.hbs && jsdoc2md --template README.hbs src/*.js > README.md && rimraf README.hbs",
    "clean": "rm -rf dist coverage",
    "example": "node examples/example.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.12.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.24.1",
    "chai": "^3.4.1",
    "eslint": "^3.4.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-mocha": "^4.5.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^2.3.4",
    "node-readme": "^0.1.9",
    "nyc": "^11.0.2",
    "uglify-js": "^2.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wickedest/myers-diff.git"
  },
  "bugs": {
    "url": "https://github.com/wickedest/myers-diff/issues"
  },
  "homepage": "https://github.com/wickedest/myers-diff",
  "license": "Apache-2.0"
}
